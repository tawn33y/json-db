{
  "name": "simple-jstore",
  "version": "0.2.9",
  "description": "simple-jstore is an in-memory database with automatic persistence to a JSON file",
  "keywords": [
    "json",
    "database",
    "in-memory",
    "store",
    "redis",
    "redux"
  ],
  "homepage": "https://github.com/tawn33y/simple-jstore",
  "bugs": {
    "url": "https://github.com/tawn33y/simple-jstore/issues",
    "email": "tawn33y@gmail.com"
  },
  "license": "GPL-3.0",
  "author": "K Tony <tawn33y@gmail.com> (https://t.me/tawn33y)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tawn33y/simple-jstore.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/index.js' --watch './dist'",
    "start": "npm-run-all clean build --parallel watch:build watch:server --print-label"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5"
  }
